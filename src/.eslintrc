{
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": 0,
    "comma-dangle": ["error", "never"],
    "max-lines-per-function": ["error", 100],
    "max-len": ["error", 200],
    "semi": ["error", "never"]
  }
}